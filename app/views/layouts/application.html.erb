<body>

<header id="header">

  <nav class="navbar navbar-inverse" role="banner">

    <div class="container">

      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="<%= WEBLINK %>"><img class="mobileLogo" src="<%= WEBLINK %>images/logo.png" alt="logo" id = "logo" style="height:120px"></a>
      </div>




            <style type="text/css">
              .navbar-nav{
                margin-top: 13%;
                font-size:20px;
              }
            </style>




      <div class="collapse navbar-collapse navbar-right">
        <ul class="nav navbar-nav">
          <li><%= link_to 'Home', {:controller=>'/welcome', :action=>'index'} %></li>
          <% eventsList = []
             Event.all.each do |event|
               if event.event_type == INDIVIDUAL or event.event_type == TEAM
                 eventsList.push(event)
               end
             end
          %>
          <li class="dropdown">
            <%= link_to 'Volunteer', {:controller=>'/registration', :action=>'volunteer_registration'}, :class=>"dropdown-toggle" %>
          </li>



          <li><%= link_to 'Contact', {:controller=>'/welcome', :action=>'contact'} %></li>
          <li><% if !user_signed_in? %>
                <li> <%= link_to('Log In', new_user_session_path)  %> </li>

                <li><%= link_to('Sign Up', new_user_registration_path)  %>
              </li>
          <% else%>
              <li>
                <%= link_to(current_user.name , edit_user_registration_path) %>
              </li>
              <li>
                <%= link_to('Sign Out', destroy_user_session_path, :method=> :delete) %>
              </li>
            <% end %>
            </ul>
      </div>
    </div><!--/.container-->
  </nav><!--/nav-->

</header><!--/header-->
<!--devise start-->
<% if notice.to_s.length > 0 %>
    <div class="notice_devise"><%= notice %></div>
<% end %>

<% if alert.to_s.length > 0 %>
    <div class="alert_devise"><%= alert %></div>
<% end %>
<!--devise end-->

<%= yield %>

